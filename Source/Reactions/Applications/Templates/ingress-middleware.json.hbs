{
{{#if IdPorten }}
    "idPorten": {
        "issuer": "{{IdPortenConfig.Issuer}}",
        "authorizationEndpoint": "{{IdPortenConfig.AuthorizationEndpoint}}"
    },
{{/if}}
    "tenants": {
{{#Tenants}}
        "{{TenantId}}": {
            "domain": "{{Domain}}",
            "onBehalfOf": "{{OnBehalfOf}}",
            "tenantIdClaims": [
            {{#TenantIdClaims}}
                "{{this}}"
            {{/TenantIdClaims}}
            ]
        }
{{/Tenants}}
    },
    "identityDetailsUrl": "{{IdentityDetailsUrl}}"
}
