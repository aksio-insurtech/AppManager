{
{{#if IdPorten }}
    "idPorten": {
        "issuer": "{{IdPortenConfig.Issuer}}",
        "authorizationEndpoint": "{{IdPortenConfig.AuthorizationEndpoint}}"
    },
{{/if}}
    "tenants": {
{{#Tenants}}
        "{{TenantId}}": {
            "domain": "{{Domain}}",
            "onBehalfOf": "{{OnBehalfOf}}",
            "sourceIdentifiers": [
            {{#SourceIdentifiers}}
                "{{this}}"{{#unless @last}},{{/unless}}
            {{/SourceIdentifiers}}
            ]
        }{{#unless @last}},{{/unless}}
{{/Tenants}}
    },
{{#if TenantResolution }}
    "tenantResolution": {
        "strategy": "{{TenantResolution.Strategy}}",
        "options": {{{TenantResolution.Options}}}
    },
{{/if}}
{{#if OAuthBearerTokenProvider }}
    "oAuthBearerTokens": {
        "authority": "{{OAuthBearerTokenProvider.Authority}}"
    },
{{/if}}
    "identityDetailsUrl": "{{IdentityDetailsUrl}}",
{{if Impersonation }}
    "impersonation": {
        "identityProviders": [
{{#IdentityProviders}}
            "aad"
{{/IdentityProviders}}
        ],
        "authorization": {
            "tenants": [
{{#Tenants}}
                "eb681eb3-f7ca-4c0b-b309-92625bff5a59"
{{/Tenants}}
            ],
            "roles": [
{{#Roles}}
                "Administrator"
{{/Roles}}
            ],
            "groups": [
{{#Groups}}
                "e3e8b1e0-1b7a-4b0a-9b0a-1b7a4b0a9b0a"
{{/Groups}}
            ],
            "claims": [
{{#Claims}}
                {
                    "type": "name",
                    "value": "Einar Ingebrigtsen"
                }
{{/Claims}}
            ]
        }
    }
{{/if}}

}
