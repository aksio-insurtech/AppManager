{
{{#if IdPorten }}
    "idPorten": {
        "issuer": "{{IdPortenConfig.Issuer}}",
        "authorizationEndpoint": "{{IdPortenConfig.AuthorizationEndpoint}}"
    },
{{/if}}
    "tenants": {
{{#Tenants}}
        "{{TenantId}}": {
            "domain": "{{Domain}}",
            "onBehalfOf": "{{OnBehalfOf}}",
            "sourceIdentifiers": [
            {{#SourceIdentifiers}}
                "{{this}}"{{#unless @last}},{{/unless}}
            {{/SourceIdentifiers}}
            ]
        }{{#unless @last}},{{/unless}}
{{/Tenants}}
    },
{{#if TenantResolution }}
    "tenantResolution": {
        "strategy": "{{TenantResolution.Strategy}}",
        "options": {{{TenantResolution.Options}}}
    },
{{/if}}
{{#if OAuthBearerTokenProvider }}
    "oAuthBearerTokens": {
        "authority": "{{OAuthBearerTokenProvider.Authority}}"
    },
{{/if}}
{{#with Impersonation}}
    "impersonation": {
        "identityProviders": [
            "aad"
        ],
        "authorization": {
            "tenants": [
{{#Tenants}}
                "{{this}}"{{#unless @last}},{{/unless}}
{{/Tenants}}
            ],
            "roles": [
{{#Roles}}
                "{{this}}"{{#unless @last}},{{/unless}}
{{/Roles}}
            ],
            "groups": [
{{#Groups}}
                "{{this}}"{{#unless @last}},{{/unless}}
{{/Groups}}
            ],
            "claims": [
{{#Claims}}
                {
                    "type": "{{this.Type}}",
                    "value": "{{this.Value}}"
                }{{#unless @last}},{{/unless}}
{{/Claims}}
            ]
        }
    },
{{/with}}
    "identityDetailsUrl": "{{IdentityDetailsUrl}}"
}
