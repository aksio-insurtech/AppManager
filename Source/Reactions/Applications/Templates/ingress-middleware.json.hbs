{
    "cookieDomain": ".aksio.app",
{{#if AzureAD }}
    "azureAd": {
        "issuer": "{{AzureAdConfig.Issuer}}",
        "authorizationEndpoint": "{{AzureAdConfig.AuthorizationEndpoint}}",
        "tokenEndpoint": "{{AzureADConfig.TokenEndpoint}}",
        "callback": "{{AzureAdConfig.Callback}}",
        "clientId": "{{AzureAdConfig.ClientId}}",
        "clientSecret": "{{AzureAdConfig.ClientSecret}}",
        "loginUrl": "{{AzureAdConfig.LoginUrl}}",
        "authName": "azuread"

    },
{{/if}}
{{#if IdPorten }}
    "idPorten": {
        "callback": "{{IdPortenConfig.Callback}}",
        "authorizationEndpoint": "{{IdPortenConfig.AuthorizationEndpoint}}",
        "tokenEndpoint": "{{IdPortenConfig.TokenEndpoint}}",
        "clientId": "{{IdPortenConfig.ClientId}}",
        "clientSecret": "{{IdPortenConfig.ClientSecret}}",
        "loginUrl": "{{IdPortenConfig.LoginUrl}}",
        "authName": "idporten"
    },
{{/if}}
    "tenants": {
{{#Tenants}}
        "{{TenantId}}": {
            "domain": "{{Domain}}",
            "onBehalfOf": "{{OnBehalfOf}}"
        }
{{/Tenants}}
    }
}
